        <div class="content">
            <div class="nav-bar">
                <img src="~/Areas/Admin/Publics/Images/menu-bars-icon-7.jpg" />
                <span class="header-title">TỔNG QUAN</span>
                <ul>
                    <li class="nav-items"><i class="far fa-clock fa-2x nav-icon"></i></li>
                    <li class="nav-items"><i class="far fa-bell fa-2x nav-icon"></i></li>
                    <li class="nav-items"><i class="far fa-user fa-2x nav-icon"></i></li>
                </ul>
            </div>

            @RenderBody()
        </div>
